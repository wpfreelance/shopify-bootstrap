{% capture function %}
  {% comment note:
    Function returns a social services feed.
    Takes one parameter which is the service url.
    Returns a block of markup containing the feed.
   %}{% endcomment %}
  
  {% capture service %}{% include 'social_service' with social_feed %}{% endcapture %}
  
  {% case service %}
  {% when 'facebook' %}
    {% capture feed %}{% include 'social_feed_facebook' with social_feed %}{% endcapture %}
    
  {% when 'instagram' %}
    {% capture feed %}{% include 'social_feed_instagram' with social_feed %}{% endcapture %}
    
  {% when 'twitter' %}
    {% capture feed %}{% include 'social_feed_twitter' with social_feed %}{% endcapture %}
  
  {% when 'vimeo' %}
    {% capture feed %}{% include 'social_feed_vimeo' with social_feed %}{% endcapture %}
  
  {% when 'youtube' %}
    {% capture feed %}{% include 'social_feed_youtube' with social_feed %}{% endcapture %}
  
  {% endcase %}
{% endcapture %}{{ feed }}{% assign feed = null %}